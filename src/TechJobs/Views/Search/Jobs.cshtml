<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml
    <ul>
            @foreach (Dictionary<string, string> row in ViewBag.jobs)
            {
                foreach (KeyValuePair <string, string> kvpair in row)
                {
                    <li>@kvpair</li>
                }
            }
        </ul>
}